<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.layout.ColumnConstraints?>
<?import javafx.scene.layout.GridPane?>
<?import javafx.scene.layout.RowConstraints?>

<AnchorPane minHeight="100.0" minWidth="250.0" xmlns="http://javafx.com/javafx/8.0.65" xmlns:fx="http://javafx.com/fxml/1" fx:controller="osrs.view.SetCompactViewController">
   <children>
      <GridPane layoutX="-16.0" layoutY="5.0" prefHeight="100.0" prefWidth="250.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="5.0" AnchorPane.rightAnchor="5.0" AnchorPane.topAnchor="5.0">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
          <ColumnConstraints hgrow="SOMETIMES" minWidth="10.0" prefWidth="100.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Label fx:id="lblSetName" text="Set:" />
            <Button mnemonicParsing="false" onAction="#handleEdit" text="Edit" GridPane.rowIndex="1" />
            <Button mnemonicParsing="false" onAction="#handleDelete" text="Delete" GridPane.rowIndex="2" />
            <Label fx:id="lblMaxHit" text="Max Hit:" GridPane.columnIndex="1" />
            <Label fx:id="lblHitChance" text="Hit Chance:" GridPane.columnIndex="1" GridPane.rowIndex="1" />
            <Label fx:id="lblDPS" text="DPS:" GridPane.columnIndex="1" GridPane.rowIndex="2" />
         </children>
      </GridPane>
   </children>
</AnchorPane>
