   <children>
      <TableView fx:id="itemTable" prefHeight="350.0" tableMenuButtonVisible="true">
        <columns>
          <TableColumn fx:id="nameColumn" prefWidth="150.0" text="Item Name" />
          <TableColumn fx:id="slotColumn" prefWidth="37.0" text="Slot" />
            <TableColumn fx:id="astabColumn" prefWidth="65.0" text="A-Stab" />
            <TableColumn fx:id="aslashColumn" prefWidth="65.0" text="A-Slash" />
            <TableColumn fx:id="acrushColumn" prefWidth="65.0" text="A-Crush" />
            <TableColumn fx:id="arangeColumn" prefWidth="65.0" text="A-Ranged" />
            <TableColumn fx:id="amagicColumn" prefWidth="65.0" text="A-Magic." />
            <TableColumn fx:id="dstabColumn" prefWidth="65.0" text="D-Stab." />
            <TableColumn fx:id="dslashColumn" prefWidth="65.0" text="D-Slash" />
            <TableColumn fx:id="dcrushColumn" prefWidth="65.0" text="D-Crush" />
            <TableColumn fx:id="drangeColumn" prefWidth="65.0" text="D-Ranged" />
            <TableColumn fx:id="dmagicColumn" prefWidth="65.0" text="D-Magic" />
            <TableColumn fx:id="strColumn" prefWidth="65.0" text="Strength" />
            <TableColumn fx:id="rstrColumn" prefWidth="65.0" text="Rng-Str" />
            <TableColumn fx:id="mdmgColumn" prefWidth="65.0" text="Mag-Dmg" />
            <TableColumn fx:id="prayColumn" prefWidth="65.0" text="Prayer" />
        </columns>
      </TableView>
      <Label layoutX="25.0" layoutY="369.0" text="Filter:" AnchorPane.bottomAnchor="10.0" AnchorPane.leftAnchor="5.0" />
      <TextField fx:id="filterField" layoutX="43.0" layoutY="366.0" AnchorPane.bottomAnchor="5.0" AnchorPane.leftAnchor="40.0" />
      <ButtonBar layoutX="885.0" layoutY="350.0" prefHeight="40.0" prefWidth="200.0" AnchorPane.bottomAnchor="5.0" AnchorPane.rightAnchor="5.0">
        <buttons>
          <Button mnemonicParsing="false" onAction="#handleOk" text="OK" />
            <Button mnemonicParsing="false" onAction="#handleClear" text="Clear" />
            <Button mnemonicParsing="false" onAction="#handleCancel" text="Cancel" />
        </buttons>
      </ButtonBar>
   </children>
</AnchorPane>
